{
    "version": 2,
    "crons": [
        {
            "path": "/api/robot/settle",
            "schedule": "0 * * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/debug",
            "destination": "/api/debug.js"
        },
        {
            "source": "/api/db-check",
            "destination": "/api/db-check.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.js"
        }
    ]
}